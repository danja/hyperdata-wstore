{
  "name": "webstore-project",
  "version": "1.0.0",
  "description": "WebStore server and client with testing suite",
  "type": "module",
  "scripts": {
    "test": "jasmine --config=jasmine.json",
    "test:server": "cd server && npm test",
    "test:client": "cd client && npm test",
    "test:e2e": "node --experimental-vm-modules ./node_modules/jasmine/bin/jasmine.js test/e2e.spec.js",
    "test:individual": "node run-individual-tests.js",
    "start:server": "cd server && npm start",
    "start:client": "cd client && npm start"
  },
  "devDependencies": {
    "jasmine": "^5.1.0",
    "mock-fs": "^5.5.0"
  }
}